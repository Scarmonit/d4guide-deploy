---
permalink: /sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%- for page in collections.all %}
{%- if not page.url | startsWith("/changepassword") and not page.url | startsWith("/kpass") and not page.url | startsWith("/upload") and not page.url | startsWith("/screen-share") %}
    <url>
        <loc>https://scarmonit.com{{ page.url }}</loc>
        <lastmod>{{ page.date | dateFormat }}</lastmod>
        <priority>{% if page.url == "/" %}1.0{% elif page.url | startsWith("/wow-tbc-guides") %}0.8{% elif page.url | startsWith("/build-guides") %}0.7{% else %}0.5{% endif %}</priority>
    </url>
{%- endif %}
{%- endfor %}
</urlset>
